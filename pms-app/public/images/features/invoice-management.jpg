<!-- Invoice Management -->

<svg xmlns="http://www.w3.org/2000/svg" width="800" height="400" viewBox="0 0 800 400">
  <rect width="800" height="400" fill="#fbe9e7"/>
  <text x="400" y="200" font-family="Arial" font-size="32" text-anchor="middle" fill="#ff5722">Invoice Management</text>
  <rect x="200" y="250" width="400" height="50" rx="10" fill="#ff5722" opacity="0.3"/>
  
  <!-- Invoice document illustration -->
  <rect x="300" y="100" width="200" height="250" fill="white" stroke="#ff8a65" stroke-width="2"/>
  
  <!-- Invoice header -->
  <rect x="300" y="100" width="200" height="40" fill="#ff8a65"/>
  <text x="400" y="125" font-family="Arial" font-size="16" text-anchor="middle" fill="white">INVOICE</text>
  
  <!-- Invoice content lines -->
  <line x1="320" y1="160" x2="480" y2="160" stroke="#ffccbc" stroke-width="2"/>
  <line x1="320" y1="190" x2="480" y2="190" stroke="#ffccbc" stroke-width="2"/>
  <line x1="320" y1="220" x2="480" y2="220" stroke="#ffccbc" stroke-width="2"/>
  <line x1="320" y1="250" x2="480" y2="250" stroke="#ffccbc" stroke-width="2"/>
  
  <!-- Invoice total section -->
  <rect x="300" y="280" width="200" height="40" fill="#ffccbc"/>
  <text x="400" y="305" font-family="Arial" font-size="16" text-anchor="middle" fill="#ff5722">TOTAL</text>
  
  <!-- Invoice shadow effect -->
  <rect x="310" y="110" width="200" height="250" fill="none" stroke="#ffccbc" stroke-width="1" opacity="0.5"/>
</svg>
