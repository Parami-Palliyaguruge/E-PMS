<!-- Analytics Use Case -->

<svg xmlns="http://www.w3.org/2000/svg" width="800" height="400" viewBox="0 0 800 400">
  <rect width="800" height="400" fill="#e0f7fa"/>
  <text x="400" y="200" font-family="Arial" font-size="32" text-anchor="middle" fill="#00bcd4">Improve Decision Making</text>
  <rect x="200" y="250" width="400" height="50" rx="10" fill="#00bcd4" opacity="0.3"/>
  
  <!-- Bar chart -->
  <rect x="250" y="130" width="40" height="100" fill="#26c6da"/>
  <rect x="300" y="80" width="40" height="150" fill="#00bcd4"/>
  <rect x="350" y="100" width="40" height="130" fill="#00acc1"/>
  <rect x="400" y="60" width="40" height="170" fill="#0097a7"/>
  <rect x="450" y="110" width="40" height="120" fill="#00838f"/>
  <rect x="500" y="140" width="40" height="90" fill="#006064"/>
  
  <!-- Line chart -->
  <polyline points="250,280 300,300 350,260 400,250 450,270 500,240 550,260" 
           fill="none" stroke="#00bcd4" stroke-width="3"/>
  
  <!-- Chart grid lines -->
  <line x1="230" y1="130" x2="230" y2="300" stroke="#b2ebf2" stroke-width="1"/>
  <line x1="230" y1="300" x2="550" y2="300" stroke="#b2ebf2" stroke-width="1"/>
  
  <!-- Data points on line -->
  <circle cx="250" cy="280" r="4" fill="#00bcd4"/>
  <circle cx="300" cy="300" r="4" fill="#00bcd4"/>
  <circle cx="350" cy="260" r="4" fill="#00bcd4"/>
  <circle cx="400" cy="250" r="4" fill="#00bcd4"/>
  <circle cx="450" cy="270" r="4" fill="#00bcd4"/>
  <circle cx="500" cy="240" r="4" fill="#00bcd4"/>
  <circle cx="550" cy="260" r="4" fill="#00bcd4"/>
</svg>
